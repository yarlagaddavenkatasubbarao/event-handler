{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "defaultDestination": "wwwroot/lib", // set default destination for libraries under wwwroot
    "libraries": [
        {
            "library": "jquery@3.4.1",
            "destination": "wwwroot/lib/jquery",
            "files": [ "jquery.min.js", "jquery.min.map" ]
        },
        {
            "library": "twitter-bootstrap@4.4.1",
            "destination": "wwwroot/lib/bootstrap",
            "files": [ "js/bootstrap.bundle.min.js", "css/bootstrap.min.css" ]
        },
        {
            "library": "jquery-validation-unobtrusive@3.2.11",
            "destination": "wwwroot/lib/jquery-validation-unobstrusive",
            "files": [ "jquery.validate.unobtrusive.min.js" ]
        },
        {
            "library": "jquery-validate@1.19.1",
            "destination": "wwwroot/lib/jquery-validate",
            "files": [ "additional-methods.js", "jquery.validate.min.js" ]
        }

    ]
}